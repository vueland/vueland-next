{
  "name": "vueland",
  "version": "1.0.0",
  "author": "Anar Allakhverdiev",
  "license": "MIT",
  "private": true,
  "description": "VueLand Components Library",
  "main": "dist/vueland.js",
  "module": "dist/vueland.js",
  "types": "types/index.d.ts",
  "sideEffects": [
    "*.sass",
    "*.scss",
    "*.css"
  ],
  "files": [
    "src/*",
    "dist/*",
    "lib/*",
    "es5/*",
    "types/*",
    "*.js",
    "*.ts"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "webpack --config build/webpack.build.conf.js",
    "build:ts": "tsc -p tsconfig.dist.json",
    "build:es5": "cross-env NODE_ENV=es5 babel lib-temp/src --out-dir es5 --source-maps",
    "build:lib": "cross-env NODE_ENV=lib babel lib-temp/src --out-dir lib --source-maps",
    "build:all": "npm run test --u & node build/build-lib.js",
    "test": "jest -u",
    "test:editor": "editorconfig-checker --exclude '.git|node_modules|dist|es5|lib|.idea'",
    "declarations": "ttsc --emitDeclarationOnly",
    "tslint-check": "tslint-config-prettier-check tslint.json",
    "prettier": "prettier --write --config .prettierrc.json './src/**/*.{ts,tsx}'",
    "lint": "eslint --ext .ts,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vueland/vueland-next.git",
    "directory": "/"
  },
  "browserslist": [
    ">1%",
    "last 5 version"
  ],
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/test-utils": "^2.0.0-rc.21",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-detective": "^2.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.4",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^5.0.1",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.24.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.0.0-beta.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.1.0",
    "jest": "^26.5.2",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-serializer-html": "^7.0.0",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^7.0.1",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^12.6.0",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^9.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "ttypescript": "^1.5.10",
    "typescript": "^4.1.3",
    "typescript-transform-paths": "^2.0.0",
    "vue-loader": "^16.1.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-remove-empty-scripts": "^0.7.3"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "vue": "^3.2.33"
  }
}
